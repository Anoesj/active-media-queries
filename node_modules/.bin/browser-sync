#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/dist/bin.js/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/dist/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/node_modules:/home/daniel/Development/active-media-queries/node_modules:/home/daniel/Development/node_modules:/home/daniel/node_modules:/home/node_modules:/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/dist/bin.js/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/dist/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/node_modules" "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/dist/bin.js/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/dist/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules/browser-sync/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/2.24.7/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/browser-sync/node_modules:/home/daniel/Development/active-media-queries/node_modules/.registry.npmjs.org/node_modules:/home/daniel/Development/active-media-queries/node_modules:/home/daniel/Development/node_modules:/home/daniel/node_modules:/home/node_modules:/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/dist/bin.js/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/dist/node_modules:/home/daniel/Development/active-media-queries/node_modules/browser-sync/node_modules" node  "$basedir/../browser-sync/dist/bin.js" "$@"
  ret=$?
fi
exit $ret
